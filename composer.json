{
    "name": "techamz/laravel-generator",
    "description": "Techamz Laravel Generator",
    "type": "library",
    "require": {
        "php": "^8.1.0",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "laracasts/flash": "^3.2.2",
        "laravelcollective/html": "^6.4",
        "symfony/var-exporter": "^6.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.7",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^8.0.0",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-laravel": "2.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Techamz\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Techamz\\Generator\\TechamzGeneratorServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Techamz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
